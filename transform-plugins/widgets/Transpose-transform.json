{
  "metadata": {
    "spec-version": "1.5"
  },
  "display-name": "Transpose",
  "configuration-groups": [
    {
      "properties": [
        {
          "widget-type": "keyvalue",
          "label": "Fields to be Mapped",
          "name": "fieldMapping",
          "widget-attributes": {
            "showDelimiter": "false"
          }
        },
	    {
          "widget-type": "ds-multiplevalues",
          "label": "Fields to be Transposed",
          "name": "fieldTransposing",
          "widget-attributes": {
            "numValues": "3",
            "placeholders": ["Transpose Field", "Field Name", "Field Value"]
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "outputSchema",
      "widget-type": "schema",
      "widget-attributes": {
        "schema-types": [
          "string"
        ],
        "schema-default-type": "string"
      }
    }
  ]
}
