{
	"metadata": {
		"spec-version": "1.5"
	},
	"configuration-groups": [
		{
			"label": "Correlation Matrix Configuration",
			"properties": [
				{
					"widget-type": "textbox",
					"name": "selectionThreshold",
					"label": "VIF Score Threshold",
					"widget-attributes": {
						"default": "0.999999999"
					},
					"plugin-function": {
						"method": "POST",
						"widget": "outputSchema",
						"output-property": "schema",
						"plugin-method": "outputSchema",
						"required-fields": [],
						"missing-required-fields-message": "'"
					}
				},
				{
					"widget-type": "textbox",
					"label": "Max VIF Iterations",
					"name": "linearRegressionIterations",
					"widget-attributes": {
						"default": "2000000000"
					}
				},
				{
					"widget-type": "textbox",
					"label": "Linear Regression Iterations",
					"name": "numIterations",
					"widget-attributes": {
						"default": "10000"
					}
				},
				{
					"widget-type": "textbox",
					"label": "Linear Regression Step Size",
					"name": "stepSize",
					"widget-attributes": {
						"default": "0.001"
					}
				},
				{
					"name": "skipEncodedFeaturesInVIF",
					"label": "Skip Encoded Features in VIF",
					"widget-type": "select",
					"widget-attributes": {
						"default": "true",
						"values": [
							"false",
							"true"
						]
					}
				},
				{
					"widget-type": "textbox",
					"label": "Number of Partitions",
					"name": "numPartitions",
					"widget-attributes": {
						"default": "600"
					}
				},
				{
					"widget-type": "textbox",
					"label": "Number of Parallel Computation Threads",
					"name": "noOfParallelThreads",
					"widget-attributes": {
						"default": "10"
					}
				}
			]
		}
	],
	"outputs": []
}
